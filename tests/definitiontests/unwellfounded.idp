vocabulary V{
	type x isa int
	p(x)
	q(x)
}

theory T:V{
	{ !x: p(x) <- q(x).
	!x: q(x) <- ~p(x).}
}

structure S:V{
	x = {0..7}
}

procedure assertions(){
	return (
		assert2(S[V::p].ct,{},"p<ct>") 
		and assert2(S[V::p].cf,{},"p<cf>") 
		and assert2(S[V::q].ct,{},"q<ct>") 
		and assert2(S[V::q].cf,{},"q<cf>") 
		and 0 --what should this be???? Inconsistent structure?
	)
}

