#include <mx>

procedure main(){
	stdoptions.nrmodels=0
	stdoptions.timeout = 5
	stdoptions.nbmodelequivalent = true
	stdoptions.groundwithbounds = false
	solutions = mx::all(T, S, stdoptions)
	io.stderr:write("Found "..#solutions.." models, expected "..getExpectedNbOfModels().." models.\n")
	if(#solutions==getExpectedNbOfModels())
		then return 1
	else return 0
	end
}