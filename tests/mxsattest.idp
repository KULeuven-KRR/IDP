#include <mx>

procedure main(){
	stdoptions.nrmodels=1
	stdoptions.timeout = 20
	stdoptions.nbmodelequivalent = true
	stdoptions.groundwithbounds = false
	solution = mx::one(T, S, stdoptions)
	if solution == nil then
		io.stderr:write("Found UNSAT, expected ")
	else
		io.stderr:write("Found SAT, expected ")
	end
	if getExpectedAModel() then
		io.stderr:write("SAT\n")
	else
		io.stderr:write("UNSAT\n")
	end
	
	if(getExpectedAModel() == (solution ~= nil))
		then return 1
	else return 0
	end
}
