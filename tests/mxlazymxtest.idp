#include <mx>

procedure main() {
	stdoptions.nbmodels = 1
	stdoptions.timeout = 50
	stdoptions.groundwithbounds = false
	stdoptions.groundlazily = true
	stdoptions.groundverbosity = 0
	stdoptions.satverbosity = 0
	
	s = sat(T,S)
	
	if s then
		io.stderr:write("Found SAT, expected ")
	else
		io.stderr:write("Found UNSAT, expected ")
	end
	
	if getExpectedAModel() then
		io.stderr:write("SAT\n")
	else
		io.stderr:write("UNSAT\n")
	end
	
	if(getExpectedAModel() == s) then
		return 1
	else
		return 0
	end
}

procedure getExpectedAModel(){
	return getExpectedNbOfModels()>0
}