vocabulary V{
	p
	type x isa int
	a:x
}

theory T:V{
}

structure S:V{
	x = {1..5}
}

#include <mx>

procedure main(){
	local expected = 10
	local sols = mx::all(T, S)
	if(not (#(sols)==expected)) then
		print("ERROR, should have been "..expected..", but found "..#(sols).." solutions.")
		return
	end
	print("OK")
}
