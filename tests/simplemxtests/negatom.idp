vocabulary V{
	p
}

theory T:V{
	~p.
}

structure S:V{
}

#include <mx>

procedure main(){
	local expected = 1

	local sols = mx::all(T, S)
	if(not #(sols)==expected) then
		print("ERROR, should have been "..expected..", but found "..#(sols).." solutions.")
		return
	end
	if(idp_intern.size(sols[1][V::p].cf)==0) then
		print("ERROR, p should have been false.")
		return
	end
	print("OK")
}
