vocabulary V{
	type x
	p(x)
}

theory T:V{
	#{x: p(x)}=2.
}

structure S:V{
	x = {1..50}
}

#include <mx>

procedure main(){
	local expected = 1
	local sols = mx::all(T, S)
	if(not (#(sols)==expected)) then
		print("ERROR, should have been "..expected..", but found "..#(sols).." solutions.")
		return
	end
	print("OK")
}
