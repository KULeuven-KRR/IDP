vocabulary V{
	type x
	p(x)
	q(x)
}

theory T:V{
	~!x: p(x) | q(x).
}

structure S:V{
	x = {1..3}
}

#include <mx>

procedure main(){
	local expected = 37
	local sols = mx::all(T, S)
	if(#(sols)==expected) then
		print("OK")
	else
		print("ERROR, should have been "..expected..", but found "..#(sols).." solutions.")
	end
}
