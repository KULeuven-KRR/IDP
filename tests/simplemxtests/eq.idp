vocabulary V{
	type x isa int
	type y isa int
	a:x
	b:y
}

theory T:V{
	a=b.
}

structure S:V{
	x = {1..5}
	y = {0..3}
}

#include <mx>

procedure main(){
	local expected = 5
	local sols = mx::all(T, S)
	if(not (#(sols)==expected)) then
		print("ERROR, should have been "..expected..", but found "..#(sols).." solutions.")
		return
	end
	print("OK")
}
