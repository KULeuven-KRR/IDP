procedure main(prover){

	stdoptions.timeout = 5
	local answer
	if prover then
		print("Not yet implemented: invariants with the prover")
		return 0
	else
		answer = isinvariant(T,invar,S)
	end
	
	if answer then
		if expectedInvariant() then
			return 1
		else
			print("Found invariant while not expecting one ")
			return 0
		end
	else
		if expectedInvariant() then
			print("Did not find invariant while expecting one ")
			return 0
		else
			return 1
		end
	end
	
	return 0	
}

