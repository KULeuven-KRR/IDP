vocabulary V{
	type action isa int
	isprereq(action, action)
	ismultprereq(action, action)
	
	type time isa int

	holds(time, action)
	
	starts(time, action)
	do(time, action)
}
vocabulary Vout{
	
}
theory T: V{
	?t: holds(t, MAX[:action]).
	{
		holds(0, MIN[:action]).
		holds(t+1, s) <- holds(t, s) | starts(t, s). 
	}
	!t a: starts(t, a) => do(t, a).
	!t a: do(t, a) => starts(t, a).
	
	!t a: do(t, a) => !a2: isprereq(a2, a) & ~ismultprereq(a2, a) => ?t2: t2<t & do(t2, a2).
	!t a: do(t, a) => ?a2: ismultprereq(a2, a) => ?a2: ismultprereq(a2, a) & ?t2: t2<t & do(t2, a2).
	
	!t: #{a: do(t, a)}<2.
		
	// prereqs: prev action, half prev action, 
	!a: action(a-1) <=> isprereq(a-1, a).
	!a a2: ismultprereq(a, a2) <=> a=a2/3 | a=a2/4.
}

structure S: V {
	time = {0..10000}
	action = {1..5}
}