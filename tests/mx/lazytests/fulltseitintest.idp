vocabulary V{
	type x
	P(x)
	P1
	P2
	P3
	P4
}

theory T:V{
	P1 => ?x: P(x).
	P2 => !x: P(x).
	P3 <=> ?x: P(x).
	P4 <=> !x: P(x).
	P3.
}

structure S:V{
	x = {1..1000000}
}

procedure main(){
	stdoptions.groundwithbounds=false
	stdoptions.groundlazily=true
	stdoptions.tseitindelay=true
	stdoptions.verbosity.solving=4
	stdoptions.verbosity.creategrounders=0; stdoptions.verbosity.grounding=0
	modelexpandpartial(T, S)
}
