vocabulary V{
	type state
	type time isa int
	type action
	initial:state
	holds(time, state)
	starts(time, state)
	do(time, action)
	
	Party:state
}

vocabulary Vout{
	
}

theory T: V{
	?t: holds(t, Party).
	{
		holds(0, initial()).
		holds(t+1, s) <- holds(t, s) | starts(t, s).
	}
	
	!t s: starts(t, s) => ?t2 t3 a a2: do(t2, a) & do(t3, a2) & a~=a2 & t2<t & t3<t.
	
	!t: #{a: do(t, a)}<2.
}

structure S: V {
	state = {1..10}
	Party = 10
	initial = 5
	time = {0..50}
	action = {1..10}
}
