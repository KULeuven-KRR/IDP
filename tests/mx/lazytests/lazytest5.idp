vocabulary V{
	type x isa int
	type y isa int
	p(x, y, x)
}

theory T: V{
	?x: !y: ?z: p(x, y, z).
	~p(1, 3, 5).
	~p(2, 1, 1).
	~p(3, 2, 2).
	~p(4, 10, 259684).
}

structure S: V {
	x = {1..1000000}
	y = {1..10}
}

#include <mx>

procedure getonesol(){
	stdoptions.groundlazily=true
	stdoptions.groundverbosity=3
	stdoptions.satverbosity=4
	return onemodel(T, S)
}

procedure main(){
--	stdoptions.groundlazy=false
--	stdoptions.language="idp"
--	idp_intern.printgrounding(T, S, stdoptions)
--	print(ground(T, S, stdoptions))
	Sol = getonesol()
	mx::printtuples("p<ct>", Sol[V::p].ct)
	mx::printtuples("p<cf>", Sol[V::p].cf)
}
