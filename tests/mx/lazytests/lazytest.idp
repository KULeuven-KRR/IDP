vocabulary V{
	type x isa int
	p(x)
}

theory T: V{
	?x: p(x).
	!x: x<5 => ~p(x).
}

structure S: V {
	x = {1..1000000}
}

#include <mx>

procedure getsomesols(){
	stdoptions.groundlazily=true
	stdoptions.verbosity.solving=4
	stdoptions.nrmodels = 10
	return modelexpand(T, S, stdoptions)
}

procedure main(){
	sols = getsomesols()
	for sol in sols
	do
		mx::printtuples("p<ct>", sol[V::p].ct)
		mx::printtuples("p<cf>", sol[V::p].cf)
	end

}
