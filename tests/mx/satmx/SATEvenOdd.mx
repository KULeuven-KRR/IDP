/************************************
        Even and Odd numbers
************************************/

vocabulary V{
type Num isa int
Even(Num)
Odd(Num)
Nul:Num


}

theory T : V { 
// Definition of even and odd
{ Even(Nul).
  Odd(x+1) <- Even(x).
  Even(x+1) <- Odd(x). }



}

