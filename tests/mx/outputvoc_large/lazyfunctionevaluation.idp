vocabulary V{
	type x isa int
	T
	p(int,int)
	isSquare(int,int)
}

vocabulary Vout{
	extern V::T/0
}

theory T:V{
	{
		p(x,y) <- isSquare(x,y).
	}
	T <=> p(5,24).
}

procedure checkSquare(x,y){
	return x*x==y;
}

structure S:V{
	isSquare = procedure checkSquare
}

procedure getExpectedNbOfModels(){
	return 1
}
