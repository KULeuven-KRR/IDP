vocabulary V{
	type x isa int
	T
	p(x,x)
	isSquare(x,x)
}

vocabulary Vout{
	extern V::T/0
}

theory T:V{
	{
		p(x,y) <- isSquare(x,y).
	}
	T <=> p(5,24).
}

procedure checkSquare(x,y){
	return x*x==y;
}

structure S:V{
	x = {-10000000..10000000}
	isSquare = procedure checkSquare
}

procedure specialOptions(){
	print("not autocompleting");
	stdoptions.autocomplete=false
}

procedure getExpectedNbOfModels(){
	return 1
}

