procedure append(a,b){
	return a..b
}

vocabulary V{
	P(string)
	app(string,string):string
}
theory T:V{
	P(app("Hello"," world")).
}
structure S:V{
	app = procedure append
}

vocabulary Vout{
	extern V::P/1
}
procedure main(){
	stdoptions.liftedunitpropagation=false
	print(modelexpand(T,S,Vout)[1])
}
