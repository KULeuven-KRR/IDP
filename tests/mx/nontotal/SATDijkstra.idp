procedure getExpectedNbOfModels(){
	return 1
}

vocabulary V{
	type Vtx
	type Length isa int
	Edge(Vtx,Vtx,Length)
 
	Sp(Vtx,Vtx,Length)
	Cp(Vtx,Vtx,Length)
}

theory T : V { 
{ 	
	Sp(x ,y ,l ) <- l = min{u : Cp(x ,y ,u) : u }.
	Cp(x ,y ,l ) <-  Edge(x ,y ,l ).
	Cp(x ,y ,l1 + l2) <- Sp(x ,y ,l1) & Edge(x ,y ,l2). 
}
}

structure S : V{ 
	Vtx = { a..b }
	Length = { 0..1 }
	Edge = {a,b,1; }
}
